package caracteres;

public class Testacaracteres {

	public static void main(String[] args) {
		
		char letra='a'; // so aceita uma letra 
		System.out.println(letra);
		char valor=66; // b maiusculo
		System.out.println(valor);
		char valor= (int)(valor+1);
		System.out.println(valor);
				
	}

}
